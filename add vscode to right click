reg add "HKEY_CLASSES_ROOT\*\shell\Open with VS Code" /ve /d "Open with VS Code" /f
reg add "HKEY_CLASSES_ROOT\*\shell\Open with VS Code\command" /ve /d "\"C:\Users\%USERNAME%\AppData\Local\Programs\Microsoft VS Code\Code.exe\" \"%1\"" /f




/to delete/ v

reg delete "HKEY_CLASSES_ROOT\*\shell\Open with VS Code" /f
